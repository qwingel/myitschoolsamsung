<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="75"
            android:background="@color/pr_green"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2">
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="4">
                </LinearLayout>
                <Button
                    android:id="@+id/profile"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:background="@drawable/profile_icon"
                    android:minHeight="16dp"
                    android:contentDescription="123"
                    tools:ignore="TouchTargetSizeCheck"></Button>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="30">
                </LinearLayout>
                <Button
                    android:id="@+id/basket"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:background="@drawable/basket_icon"
                    android:minHeight="16dp"
                    android:contentDescription="123"
                    tools:ignore="TouchTargetSizeCheck"></Button>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="4">
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2">
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/white_round_up"
                android:layout_weight="2">
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="244"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1">
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="10"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/tickets_search"
                    android:textSize="32sp"
                    android:textColor="#000000"
                    android:fontFamily="@font/raleway_medium"
                    android:gravity="center">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="8"
                    android:background="@drawable/tickets_lin_background"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="12"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="2">
                        </LinearLayout>

                        <AutoCompleteTextView
                            android:id="@+id/from_Where"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="10"
                            android:alpha="0.6"
                            android:background="@drawable/edittext"
                            android:fontFamily="@font/raleway"
                            android:hint="@string/fromwhere"
                            android:paddingLeft="15dp"
                            android:textColorHint="#000000"
                            android:textSize="18sp"
                            tools:ignore="TouchTargetSizeCheck"></AutoCompleteTextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.3"
                            android:background="@drawable/poloska">
                        </LinearLayout>
                        <AutoCompleteTextView
                            android:id="@+id/to_Where"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="10"
                            android:alpha="0.6"
                            android:background="@drawable/edittext"
                            android:fontFamily="@font/raleway"
                            android:hint="@string/towhere"
                            android:paddingLeft="14dp"
                            android:textColorHint="#000000"
                            android:textSize="18sp"
                            tools:ignore="TouchTargetSizeCheck"></AutoCompleteTextView>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="2">
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.4">
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">
                    <Button
                        android:id="@+id/calendar"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="6"
                        android:background="@drawable/tickets_lin_background"
                        android:minHeight="0dp"
                        android:orientation="horizontal"
                        android:text="Даты"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/raleway"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:drawableStart="@drawable/calendar"
                        tools:ignore="TouchTargetSizeCheck">
                    </Button>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5">
                    </LinearLayout>
                    <Button
                        android:id="@+id/countPass"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="9"
                        android:background="@drawable/tickets_lin_background"
                        android:minHeight="0dp"
                        android:text="Кол-во чел"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/raleway"
                        android:gravity="center"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:drawableStart="@drawable/countpass"
                        tools:ignore="TouchTargetSizeCheck"></Button>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5">
                    </LinearLayout>

                    <Button
                        android:id="@+id/filters"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="7.5"
                        android:background="@drawable/tickets_lin_background"
                        android:fontFamily="@font/raleway"
                        android:gravity="center"
                        android:minHeight="0dp"
                        android:text="Фильтры"
                        android:textColor="#000000"
                        android:textSize="14sp"
                        android:drawableStart="@drawable/filters"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        tools:ignore="TouchTargetSizeCheck"></Button>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2">
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1">
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="63"
            android:background="@drawable/tickets_lin_background"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/textViewFromTo"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:fontFamily="@font/raleway_medium"
                    android:textSize="20sp"
                    android:textColor="#000000"
                    android:text="@string/choise_Cities"
                    android:paddingLeft="50dp">
                </TextView>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <ListView
            android:id="@+id/list_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="430">
        </ListView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>